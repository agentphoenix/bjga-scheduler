{
	"require": {
		"laravel/framework": "4.1.*",
		"dflydev/markdown": "~1.0",
		"iron-io/iron_mq": "1.5.*",
		"sabre/vobject" : "3.1.*",
		"ikimea/browser": "~1.0",
		"indatus/dispatcher": "~1.0",
		"laracasts/presenter": "0.1.*",
		"raveren/kint": "dev-master"
	},
	
	"require-dev": {
		"fzaninotto/faker": "~1.0"
	},
	
	"autoload": {
		"classmap": [
			"Scheduler/database/migrations",
			"Scheduler/database/seeds"
		],
		
		"psr-4": {
			"Scheduler\\": "Scheduler"
		}
	},
	
	"config": {
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}