{
	"require": {
		"laravel/framework": "4.2.*",
		"iron-io/iron_mq": "1.5.*",
		"sabre/vobject" : "3.2.*",
		"ikimea/browser": "~1.0",
		"laracasts/presenter": "~0.1",
		"raveren/kint": "dev-master",
		"dingo/api": "~0.2",
		"league/commonmark": "*"
	},
	
	"require-dev": {
		"fzaninotto/faker": "~1.0",
		"barryvdh/laravel-debugbar": "1.*"
	},
	
	"autoload": {
		"classmap": [
			"BrianJacobs/Scheduler/database/migrations",
			"BrianJacobs/Scheduler/database/seeds"
		],
		
		"psr-4": {
			"Scheduler\\": "BrianJacobs\\Scheduler\\Site",
			"Scheduler\\Api\\": "BrianJacobs\\Scheduler\\Api",
			"Scheduler\\Extensions\\": "BrianJacobs\\Scheduler\\Extensions",
			"Plans\\": "BrianJacobs\\Scheduler\\Plans"
		}
	},
	
	"config": {
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}
