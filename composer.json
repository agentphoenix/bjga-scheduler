{
	"require": {
		"laravel/framework": "4.0.*",
		"dflydev/markdown": "1.*",
		"iron-io/iron_mq": "1.*"
	},
	
	"require-dev": {
		"raveren/kint": "dev-master"
	},
	
	"autoload": {
		"classmap": [
			"app/database/migrations",
			"app/database/seeds"
		],
		
		"psr-0": {
			"Scheduler\\": "app/"
		}
	},
	
	"config": {
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}